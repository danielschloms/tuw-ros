{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "inputs": [
        {
          "type": "pickString",
          "id": "pkgs",
          "description": "Which ros pkg do like?",
          "options": [
            "fake_localization",
            "graph_server",
          ],
          "default": "graph_server"
        },
        {
          "type": "pickString",
          "id": "ws",
          "description": "Which worksapce do like?",
          "options": [
            "ws00",
            "ws01",
            "ws02",
          ],
          "default": "ws02"
        },
    ],
    "configurations": [
        {
            "name": "stage_ros2 cave",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/stage_ros2/stage_ros2",
            "args": ["--ros-args", "-p", "world_file:=hallway.world", "-p", "enable_gui:=true"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/ws01/src/stage_ros2/world/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "demo_graph_io",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph/lib/tuw_graph/demo_graph_io",
            "args": [""],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "demo_skeleon_to_graph",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph/lib/tuw_graph/demo_skeleon_to_graph",
            "args": [""],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "voronoi_demo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph/lib/tuw_graph/demo_voronoi",
            "args": ["ws02/src/tuw_graph/tuw_graph/res/map_occupancy_grid.png"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_graph_search astar",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph_search/lib/tuw_graph_search/demo_astar",
            "args": ["ws02/src/tuw_graph/tuw_graph/res/map_occupancy_grid.png"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "laserscan_linedetection",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/install/tuw_laserscan_features/lib/tuw_laserscan_features/linedetection",
            "args": ["--ros-args", "-r", "scan:=base_scan"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_json example_json",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_json/example_json",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_json unittest",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_json/unittest",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_msgs test_std_msgs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_msgs/test_std_msgs",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_msgs test_graph_msgs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_msgs/test_graph_msgs",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_msgs test_graph_json",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_msgs/test_graph_json",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "laserscan_features",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/install/tuw_laserscan_features/lib/tuw_laserscan_features/composed_node",
            "args": ["--ros-args", "-r", "scan:=base_scan"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "feature_viz_linedetection",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/install/tuw_laserscan_features/lib/tuw_laserscan_features/feature_viz",
            "args": ["--ros-args", "-r", "scan:=base_scan"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_graph_server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph_server/lib/tuw_graph_server/graph_server_node",
            "args": ["--ros-args", "--params-file", "./ws02/src/tuw_graph/tuw_graph_server/config/graph_server/cave/graph.yaml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_graph_dense",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph/lib/tuw_graph/demo_dense_graph",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_graph_saver",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_graph_server/lib/tuw_graph_server/graph_saver_node",
            "args": ["--ros-args", "filename", "graph-captured.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "tuw_geometry_test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws01/build/tuw_geometry/test_geometry",
            "args": [""],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "nav2_planner",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws00/build/nav2_planner/planner_server",
            "args": ["--ros-args", "--params-file", "ws02/src/tuw_planner/tuw_planner_graph/config/nav2/astar.yaml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "ObjectMap publisher",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_greenhive/lib/tuw_greenhive/object_map_publisher",
            "args": ["--ros-args", "-p", "publishing_rate:=2.0", "-p", "filename:=${workspaceFolder}/ws02/src/tuw_greenhive/config/object_map/dummy.json"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "ObjectMap to CostMap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/install/tuw_object_map/lib/tuw_object_map/generate_object_costmap",
            "args": ["--ros-args",  "-p", "json_file:=${workspaceFolder}/ws02/src/tuw_greenhive/config/object_map/straden.json"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "run pkg",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/${input:ws}/install/tuw_${input:pkgs}/lib/tuw_${input:pkgs}/${input:pkgs}_node",
            "args": ["--ros-args", "-r", "scan:=base_scan"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Python: move",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/src/mr_move/mr_move/move.py",
            "args": ["--ros-args", "-r", "scan:=base_scan"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "graph_publisher_node",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ws02/src/tuw_greenhive/scripts/graph_publisher_node.py",
            "args": ["${workspaceFolder}/ws02/src/tuw_greenhive/config/gh/nodes.json", "${workspaceFolder}/ws02/src/tuw_greenhive/config/gh/edges.json"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}